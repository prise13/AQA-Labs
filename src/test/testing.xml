    <parameter name="first-value" value="10"/>
    <parameter name="second-value" value="2"/>
    <parameter name="expected-result" value="5"/>
    <test name="CalculatorTest" >
        <classes>
            <class name="aqa_tests.AddClassTest" />
            <class name="aqa_tests.DivisionCalcTest" />
            <class name="aqa_tests.MinusCalcTest" />
            <class name="aqa_tests.MultipleClassTest" />
        </classes>
    </test>
</suite>
